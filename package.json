{
  "name": "@kyletaylored/datadog-rum-interceptor",
  "version": "0.0.6",
  "author": "Kyle Taylor <kyle.taylor@datadoghq.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyletaylored/datadog-rum-interceptor.git"
  },
  "files": [
    "src",
    "dist",
    "!.github/",
    "!test/"
  ],
  "main": "dist/node/datadog-rum-interceptor.node.cjs.js",
  "module": "dist/node/datadog-rum-interceptor.node.es.js",
  "browser": "dist/browser/datadog-rum-interceptor.browser.umd.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/datadog-rum-interceptor.node.es.js",
        "require": "./dist/node/datadog-rum-interceptor.node.cjs.js"
      },
      "browser": "./dist/browser/datadog-rum-interceptor.browser.umd.js",
      "default": "./dist/node/datadog-rum-interceptor.node.es.js"
    }
  },
  "scripts": {
    "build:browser": "vite build --config vite.config.browser.js",
    "build:node": "vite build --config vite.config.node.js",
    "build": "npm run build:browser && npm run build:node",
    "prepare": "npm run build",
    "start": "npm run build && cp ./dist/browser/*.js /Users/kyle.taylor/server/demo/datadog-demo-app/frontend/public/"
  },
  "keywords": [
    "interceptor",
    "request",
    "datadog",
    "rum",
    "vite"
  ],
  "dependencies": {
    "@mswjs/interceptors": "^0.37.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16",
    "vite": "^6",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.32.1"
  }
}
