{
  "name": "@kyletaylored/datadog-rum-interceptor",
  "description": "A lightweight request/response interceptor to provide payload visibility for debugging and monitoring.",
  "version": "0.0.7",
  "author": "Kyle Taylor <kyle.taylor@datadoghq.com>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyletaylored/datadog-rum-interceptor.git"
  },
  "files": [
    "src",
    "dist",
    "!.github/",
    "!test/"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./dist/es/main.js",
        "require": "./dist/cjs/main.js"
      },
      "browser": "./dist/umd/main.js",
      "default": "./dist/es/main.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "prepare": "npm run build",
    "start": "vite build --watch --mode development",
    "help": "vite --help"
  },
  "keywords": [
    "interceptor",
    "request",
    "datadog",
    "rum",
    "vite"
  ],
  "dependencies": {
    "@mswjs/interceptors": "^0.37.5"
  },
  "devDependencies": {
    "vite": "^6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.32.1"
  }
}
